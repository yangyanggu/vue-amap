{
  "private": true,
  "packageManager": "pnpm@6.23.6",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "pnpm run build:util & pnpm run build:amap",
    "build:amap": "gulp --require sucrase/register/ts -f build/gulp/amapGulpFile.ts",
    "build:loca": "gulp --require sucrase/register/ts -f build/gulp/locaGulpFile.ts",
    "build:extra": "gulp --require sucrase/register/ts -f build/gulp/extraGulpFile.ts",
    "build:util": "gulp --require sucrase/register/ts -f build/gulp/utilGulpFile.ts",
    "dev": "pnpm run -C test dev",
    "clean:dist": "rimraf dist",
    "lint": "eslint src",
    "docs:dev": "pnpm run -C docs dev",
    "docs:build": "pnpm run -C docs build"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
  },
  "devDependencies": {
    "@pnpm/find-workspace-packages": "3.1.28",
    "@pnpm/logger": "4.0.0",
    "@pnpm/types": "7.6.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@turf/turf": "^6.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/gulp": "4.0.9",
    "@types/jest": "27.0.2",
    "@types/lodash-es": "4.17.5",
    "@types/sass": "1.43.1",
    "@types/three": "0.143.0",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "@vue/compiler-sfc": "3.2.37",
    "components-helper": "^1.0.5",
    "esbuild": "0.14.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-define-config": "1.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-vue": "8.7.1",
    "fast-glob": "3.2.7",
    "fs-extra": "10.0.0",
    "gulp": "4.0.2",
    "magic-string": "^0.27.0",
    "rimraf": "3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-esbuild": "4.7.2",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-scss": "3.0.0",
    "rollup-plugin-vue": "6.0.0",
    "sass": "^1.45.0",
    "sucrase": "^3.20.3",
    "ts-morph": "14.0.0",
    "typescript": "^4.7.4",
    "vue": "^3.2.37",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
